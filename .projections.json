{
    "src/Discord/*.hs": {
        "alternate": "test/{}Spec.hs",
        "type": "src",
        "template": [
            "{- |",
            "Copyright: (c) 2021 Reyu Zenfold",
            "SPDX-License-Identifier: MIT",
            "Maintainer: Reyu Zenfold <reyu@reyuzenfold.com>",
            "",
            "-}",
            "",
            "module Discord.{dot}",
            "    () where"
        ]
    },
    "src/Discord.hs": {
        "alternate": "test/{}Spec.hs",
        "type": "src"
    },
    "test/*Spec.hs": {
        "alternate": "src/{}.hs",
        "type": "test"
    },
    "*": {
        "console": "cabal repl",
        "make": "nix-build"
    }
}
